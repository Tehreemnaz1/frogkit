{
  "name": "frogkit",
  "version": "0.0.1",
  "description": "Smartfrog Component Framework",
  "scripts": {
    "storybook": "start-storybook -p 5000 -c .storybook",
    "build-storybook": "build-storybook",
    "precommit": "prettier --config .prettierrc '{,!(node_modules)/**/}*.js' --write",
    "test": "jest"
  },
  "dependencies": {},
  "peerDependencies": {
    "vue": "^2.5.2"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.2.13",
    "@storybook/addon-knobs": "3.2.13",
    "@storybook/vue": "^3.2.13",
    "babel-jest": "21.2.0",
    "jest": "21.2.1",
    "jest-serializer-vue": "0.2.0",
    "jest-vue": "0.8.1",
    "prettier": "1.7.4",
    "stylus": "0.54.5",
    "stylus-loader": "3.0.1",
    "vue": "2.5.2",
    "vue-loader": "13.3.0",
    "vue-template-compiler": "2.5.2",
    "vue-test-utils": "1.0.0-beta.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.(vue)$": "<rootDir>/node_modules/jest-vue",
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "snapshotSerializers": [
      "<rootDir>/node_modules/jest-serializer-vue"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(spec))\\.jsx?$",
    "mapCoverage": true
  }
}
