.demo
  background #f18713
  border #7A7A7A 1px solid
  text-align center
  height 100px
  line-height 100px
  font-size 20px

.flex-demo
  background #f18713
  border #7A7A7A 1px solid
  text-align center
  min-height 100px
  min-width 100px
  line-height 100px
  font-size 20px
  &:nth-child(even)
    background #2f82c4
    min-height 200px
    min-width 200px
  &:nth-child(3n)
    background #3DA35D
    min-height 150px
    min-width 150px
