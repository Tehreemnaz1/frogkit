margin-mixin($margin)
  .m-{$margin}
    margin: $margin
  .m-t-{$margin}
    margin-top: $margin
  .m-r-{$margin}
    margin-right: $margin
  .m-b-{$margin}
    margin-bottom: $margin
  .m-l-{$margin} 
    margin-left: $margin

margin-factory(n)
  for num in (1..n)
    $margin = unit((num * 10), 'px')
    margin-mixin($margin)

margin-factory(5)
margin-mixin(15px)